<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.test.demo.mapper.MemberMapper">
    <insert id="insert_member" parameterType="com.test.demo.vo.MemberVO">
        INSERT INTO member(kakao, email, name)
        VALUES(#{kakao}, #{email}, #{name})
    </insert>
</mapper>